[Unit]
Description=Daily timer for Carlo notification scheduling
# After=default.target

[Timer]
OnCalendar=daily
Persistent=true
# Alternatively run at a specific time each day:
# OnCalendar=*-*-* 09:00:00
# RandomizedDelaySec=1h
# AccuracySec=5m
# Unit=claro-notification-scheduler.service

[Install]
WantedBy=timers.target
