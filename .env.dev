# API Keys
GROK_API_KEY=
ZEP_API_KEY=

# Memory provider: "zep" or "mock"
MEMORY_PROVIDER=mock

# Zep settings
ZEP_API_URL=

# User settings
ZEP_USER_ID=
ZEP_USER_FIRST_NAME=Claro
ZEP_USER_LAST_NAME=User
ZEP_USER_EMAIL=

# Server
HOST=0.0.0.0
PORT=8000
CORS_ORIGINS=http://localhost:5173,http://localhost:5174,http://localhost:8000

# LLM
LLM_MODEL=grok-4-latest
LLM_TEMPERATURE=2.0
LLM_MAX_TOKENS=2000

# Session
MAX_SESSION_MESSAGES=500
SESSION_TIMEOUT_HOURS=72

# LangSmith (optional)
LANGCHAIN_TRACING_V2=false
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=Claro-Agent

# Logging
LOG_LEVEL=INFO
